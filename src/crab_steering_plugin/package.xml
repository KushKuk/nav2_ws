<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>crab_steering_plugin</name>
  <version>0.0.0</version>
  <description>Crab steering plugin for Ignition Gazebo</description>
  <maintainer email="kukrejakush@gmail.com">kush</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

<depend>ros_gz_cmake</depend>

<build_depend>rclcpp</build_depend>
<build_depend>rclpy</build_depend>
<build_depend>geometry_msgs</build_depend>
<build_depend>nav_msgs</build_depend>
<build_depend>tf2_ros</build_depend>
<build_depend>ros_gz_sim</build_depend>
<build_depend>ros_gz_bridge</build_depend>

<exec_depend>rclcpp</exec_depend>
<exec_depend>rclpy</exec_depend>
<exec_depend>geometry_msgs</exec_depend>
<exec_depend>nav_msgs</exec_depend>
<exec_depend>tf2_ros</exec_depend>
<exec_depend>ignition-gazebo</exec_depend>
<exec_depend>ros_gz_sim</exec_depend>
<exec_depend>ros_gz_bridge</exec_depend>


  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
