<gazebo reference="zed_right_camera_frame">
  <sensor name="zed_right_camera" type="camera">
    <always_on>true</always_on>
    <update_rate>30</update_rate>
    <visualize>true</visualize>
    <camera>
      <horizontal_fov>1.3962634</horizontal_fov>
      <image>
        <width>1280</width>
        <height>720</height>
        <format>R8G8B8</format>
      </image>
      <clip>
        <near>0.1</near>
        <far>50.0</far>
      </clip>
    </camera>
    <plugin name="ros2_right_cam" filename="libgazebo_ros_camera.so">
      <ros>
        <namespace>/zed2i</namespace>
        <argument>right/image_raw</argument>
      </ros>
    </plugin>
  </sensor>
<!-- ========= /ZED2 ========= -->
